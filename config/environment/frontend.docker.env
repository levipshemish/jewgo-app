# Docker Environment Configuration for Frontend

# Supabase Configuration (using placeholder values for Docker)
NEXT_PUBLIC_SUPABASE_URL=https://placeholder.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=placeholder-anon-key
SUPABASE_SERVICE_ROLE_KEY=placeholder-service-role-key

# Backend URL (pointing to Docker backend service)
NEXT_PUBLIC_BACKEND_URL=http://backend:5000
NEXT_PUBLIC_API_URL=http://backend:5000

# Environment
NODE_ENV=production
DOCKER=true

# Feature Flags
NEXT_PUBLIC_FEATURE_FLAGS={}

# Disable Supabase Realtime for Docker
NEXT_PUBLIC_SUPABASE_REALTIME_ENABLED=false

# Analytics Configuration
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Development Configuration
ANALYZE=false

# Cookie HMAC Key (required for production)
MERGE_COOKIE_HMAC_KEY_CURRENT=placeholder-hmac-key-for-docker

# Upstash Redis Configuration (placeholder values for Docker)
UPSTASH_REDIS_REST_URL=https://placeholder.upstash.io
UPSTASH_REDIS_REST_TOKEN=placeholder-token-for-docker

# Disable rate limiting in Docker for development
NEXT_PUBLIC_RATE_LIMITING_ENABLED=false
