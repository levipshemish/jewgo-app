{
  "dashboard": {
    "id": null,
    "title": "JewGo Application Overview",
    "description": "Comprehensive overview of JewGo application metrics and performance",
    "tags": ["jewgo", "overview", "application"],
    "style": "dark",
    "timezone": "utc",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Application Health",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "up{job=~\"jewgo-.*\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Response Time P95",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 2 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "expr": "rate(http_requests_total[1m])",
            "legendFormat": "{{job}} - {{method}} {{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 7,
        "title": "Database Performance",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 12 },
        "targets": [
          {
            "expr": "rate(jewgo_database_queries_total[5m])",
            "legendFormat": "Queries/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(jewgo_database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Query P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Search Performance",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 12 },
        "targets": [
          {
            "expr": "rate(jewgo_search_requests_total[5m])",
            "legendFormat": "Search Requests/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(jewgo_search_duration_seconds_bucket[5m]))",
            "legendFormat": "Search P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Cache Performance",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 16, "y": 12 },
        "targets": [
          {
            "expr": "rate(jewgo_cache_hits_total[5m]) / rate(jewgo_cache_requests_total[5m]) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "green", "value": 90 }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "User Activity",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 12, "x": 0, "y": 18 },
        "targets": [
          {
            "expr": "rate(jewgo_user_actions_total[5m])",
            "legendFormat": "User Actions/sec"
          },
          {
            "expr": "rate(jewgo_users_active_total[5m])",
            "legendFormat": "Active Users/sec"
          }
        ]
      },
      {
        "id": 11,
        "title": "Business Metrics",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 12, "x": 12, "y": 18 },
        "targets": [
          {
            "expr": "rate(jewgo_restaurant_searches_total[5m])",
            "legendFormat": "Restaurant Searches/sec"
          },
          {
            "expr": "rate(jewgo_synagogue_searches_total[5m])",
            "legendFormat": "Synagogue Searches/sec"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up, instance)",
          "refresh": 1
        },
        {
          "name": "job",
          "type": "query",
          "query": "label_values(up, job)",
          "refresh": 1
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(process_start_time_seconds[1h]) > 0"
        }
      ]
    }
  }
}
