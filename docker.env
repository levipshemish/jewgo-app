# Docker Environment Configuration
# Copy this file to .env and update with your actual values

# --- Database Configuration ---
POSTGRES_DB=jewgo_db
POSTGRES_USER=jewgo_user
POSTGRES_PASSWORD=your_secure_password_here
DATABASE_URL=postgresql://jewgo_user:your_secure_password_here@postgres:5432/jewgo_db

# --- Redis Configuration ---
REDIS_PASSWORD=your_redis_password_here
REDIS_URL=redis://:your_redis_password_here@redis:6379

# --- Application Configuration ---
NODE_ENV=production
FLASK_ENV=production
SECRET_KEY=your_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key_here

# --- CORS Configuration ---
CORS_ORIGINS=https://jewgo.app,https://www.jewgo.app,https://api.jewgo.app,http://localhost:3000,http://127.0.0.1:3000

