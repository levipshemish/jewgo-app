{
  "enabled": true,
  "level": "INFO",
  "enableConsole": true,
  "enableFile": true,
  "logFile": "logs/scripts.log",
  "enableJson": false,
  "jsonFile": "logs/scripts.json",
  "maxFileSize": 10485760,
  "maxFiles": 5,
  "enableProgress": true,
  "enableTiming": true,
  "enableMetrics": true,
  "environments": {
    "development": {
      "level": "DEBUG",
      "enableFile": false,
      "enableProgress": true
    },
    "production": {
      "level": "INFO",
      "enableFile": true,
      "enableJson": true,
      "enableProgress": false
    }
  }
}