# Database Configuration
DATABASE_URL="your_database_url"

# App Configuration
NEXT_PUBLIC_APP_VERSION=your_app_version
NEXT_PUBLIC_ENV=your_environment

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
NEXT_PUBLIC_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id

# Backend API
NEXT_PUBLIC_BACKEND_URL=your_backend_url

# Supabase Configuration (Required)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Authentication (Required)
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=your_nextauth_url

# Debug Configuration
NEXT_PUBLIC_DEBUG_PLACES_BADGE=your_debug_places_badge

# Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=your_ga_measurement_id

# Sentry (Optional)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Cloudinary Configuration (Optional)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Admin Configuration
NEXT_PUBLIC_ADMIN_EMAIL=admin@example.com

# reCAPTCHA (Optional)
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Cron Jobs (Required in production)
CLEANUP_CRON_SECRET=your_cleanup_cron_secret

# Cloudflare Turnstile
NEXT_PUBLIC_TURNSTILE_SITE_KEY=your_turnstile_site_key
TURNSTILE_SECRET_KEY=your_turnstile_secret_key
