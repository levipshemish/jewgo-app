{
  "name": "jewgo-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build && node scripts/post-build-fix.js",
    "export": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "health-check": "node scripts/health-check.js",
    "validate-env": "node scripts/validate-env.js && node scripts/validate-supabase-env.js",
    "check-env": "node scripts/check-environment.js",
    "deploy:check": "npm run validate-env && npm run health-check && echo 'âœ… Deployment validation complete'",
    "deploy:validate": "node scripts/deploy-validate.js",
    "deploy:setup": "node scripts/deploy-setup.js",
    "deploy:vercel": "node scripts/deploy-setup.js --vercel",
    "deploy:render": "node scripts/deploy-setup.js --render",
    "monitor:health": "node scripts/health-monitor.js",
    "monitor:setup": "node scripts/setup-monitoring.js",
    "monitor:test": "node scripts/test-monitoring.js",
    "seed:data": "node scripts/seed-test-data.js",
    "optimize:apiProcedure": "node scripts/optimize-api.js",
    "analyze": "ANALYZE=true npm run build",
    "security:audit": "npm audit && npm audit fix",
    "security:fix": "npm audit fix --force",
    "bundle:analyze": "npm run build && npx @next/bundle-analyzer",
    "performance:check": "npm run build && npx lighthouse --output=json --output-path=./lighthouse-report.json",
    "monitor:start": "node scripts/health-monitor.js start",
    "monitor:check": "node scripts/health-monitor.js check",
    "monitor:report": "node scripts/health-monitor.js report",
    "monitor:rotate-logs": "node scripts/rotate-logs.js",
    "monitor:aggregate": "node scripts/aggregate-metrics.js",
    "monitor:cleanup": "node scripts/cleanup-logs.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "start:production": "NODE_ENV=production npm run start",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage",
    "mendel:phase-track": "bash ../ci-scripts/phase_tracking.sh",
    "mendel:temp-deprecated-check": "node ../ci-scripts/temp_deprecated_check.js",
    "mendel:dup-scan": "node ../ci-scripts/dup_scan.js",
    "mendel:root-check": "bash ../ci-scripts/root_litter_check.sh",
    "mendel:deprecation-check": "bash ../ci-scripts/deprecation_check.sh",
    "mendel:coverage-gate": "node ../ci-scripts/coverage_gate.js 80 70 coverage/coverage-summary.json",
    "mendel:performance-budget": "node ../ci-scripts/performance_budget.js",
    "mendel:performance-regression": "node ../ci-scripts/performance_regression.js current-metrics.json baseline-metrics.json",
    "mendel:db-performance": "node ../ci-scripts/db_performance_check.js",
    "mendel:deprecation-ownership": "node ../ci-scripts/deprecation_ownership_check.js",
    "mendel:context7-validation": "node ../ci-scripts/context7_validation.js",
    "mendel:duplication-prevention": "node ../ci-scripts/duplication_prevention.js",
    "mendel:rollback-validation": "node ../ci-scripts/rollback_validation.js",
    "mendel:file-placement": "node ../ci-scripts/file_placement_check.js",
    "mendel:pr-template-validation": "node ../ci-scripts/pr_template_validation.js",
    "mendel:all": "npm run mendel:phase-track && npm run mendel:temp-deprecated-check && npm run mendel:dup-scan && npm run mendel:root-check && npm run mendel:deprecation-check && npm run mendel:coverage-gate && npm run mendel:performance-budget && npm run mendel:performance-regression && npm run mendel:db-performance && npm run mendel:deprecation-ownership && npm run mendel:context7-validation && npm run mendel:duplication-prevention && npm run mendel:rollback-validation && npm run mendel:file-placement && npm run mendel:pr-template-validation",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:images:enhanced": "node scripts/enhanced-image-optimizer.js",
    "replace:images": "node scripts/replace-original-images.js",
    "optimize:bundles": "node scripts/optimize-bundles.js",
    "optimize:css": "node scripts/optimize-css.js",
    "optimize:performance": "node scripts/performance-optimization.js",
    "performance:audit": "npm run build && npx lighthouse --output=json --output-path=./performance-audit.json",
    "performance:report": "node scripts/generate-performance-report.js",
    "test:fonts": "node scripts/test-font-loading.js",
    "validate:css": "node scripts/validate-css.js",
    "fix:font-css": "node scripts/fix-font-css.js",
    "test:prisma": "node scripts/test-prisma-config.js",
    "test:auth-systems": "tsx scripts/test-auth-systems.ts",
    "migrate:users": "tsx scripts/migrate-users.ts",
    "supabase:test": "tsx scripts/test-auth-systems.ts",
    "supabase:migrate": "tsx scripts/migrate-users.ts",
    "check:env": "tsx scripts/check-env-setup.ts",
    "generate:secret": "tsx scripts/generate-secret.ts"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@prisma/client": "^5.22.0",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@swc/helpers": "^0.5.17",
    "@types/leaflet": "^1.9.20",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.6",
    "framer-motion": "^12.23.12",
    "hoist-non-react-statics": "^3.3.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.536.0",
    "next": "^15.4.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "otplib": "^12.0.1",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^19.1.1",
    "react-leaflet": "^4.2.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@humanwhocodes/config-array": "^0.13.0",
    "@humanwhocodes/object-schema": "^2.0.3",
    "@next/bundle-analyzer": "^14.2.30",
    "@squoosh/cli": "^0.7.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.8.0",
    "node-loader": "^2.1.0",
    "prettier": "^3.2.4",
    "prisma": "^5.22.0",
    "sharp": "^0.34.3",
    "terser-webpack-plugin": "^5.3.10",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "cookie": "^0.7.0",
    "rimraf": "^5.0.0",
    "glob": "^10.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11"
  ],
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.0",
    "glob": "^10.0.0",
    "@humanwhocodes/config-array": "^0.13.0",
    "@humanwhocodes/object-schema": "^2.0.3"
  }
}
