  zip_code           String?   @db.VarChar
  rabbi              String?   @db.VarChar
  affiliation        String?   @db.VarChar
  phone              String?   @db.VarChar
  email              String?   @db.VarChar
  website            String?   @db.VarChar
  social_media       String?
  shacharit          String?
  mincha             String?
  maariv             String?
  shabbat            String?
  sunday             String?
  weekday            String?
