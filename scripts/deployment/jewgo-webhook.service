[Unit]
Description=Jewgo App GitHub Webhook Handler
After=network.target

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/jewgo-app
ExecStart=/usr/bin/python3 /home/ubuntu/jewgo-app/scripts/deployment/webhook-handler.py
Restart=always
RestartSec=10
Environment=WEBHOOK_SECRET=your-webhook-secret-here

[Install]
WantedBy=multi-user.target
